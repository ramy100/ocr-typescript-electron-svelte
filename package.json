{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "main": "electron/build/main.js",
  "scripts": {
    "build": "rollup -c",
    "svelte:dev": "rollup -c -w",
    "start": "sirv public --no-clear --single",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "concurrently \"npm run svelte:dev\" \"npm run watch:ts\" \"npm run electron:dev\" ",
    "electron:dev": "electron ./electron/build/main.js",
    "watch:ts": "tsc -p ./electron -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "electron": "13.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.34.1",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "concurrently": "^6.2.0",
    "electron-reloader": "^1.2.1",
    "node-sass": "^6.0.0",
    "pdf-poppler": "^0.2.1",
    "sirv-cli": "^1.0.0",
    "svelte-navigator": "^3.1.5",
    "tesseract.js": "^2.1.1"
  }
}
